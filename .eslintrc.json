{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends":["plugin:react/recommended", "airbnb-base","airbnb/hooks"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "import/first": "off",
    "import/no-extraneous-dependencies": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off"
  }
}
